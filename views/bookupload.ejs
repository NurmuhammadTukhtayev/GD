                                                <label>Upload BookImage</label>
                                                <input type="file" class="form-control" name="myImage" />
                                                <input type="submit" value="Upload"/>
                                            </form>
                                    </div>
                                </div>
                            </div>
         <!-- /. PAGE WRAPPER  -->
        </div>
                    </div>
